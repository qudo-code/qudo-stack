{
  "name": "docker",
  "scripts": {
    "up": "dotenv -e ../../.env -- docker compose up -d",
    "down": "dotenv -e ../../.env -- docker compose down --remove-orphans --volumes",
    "start": "dotenv -e ../../.env -- docker compose start",
    "stop": "dotenv -e ../../.env -- docker compose stop"
  }
}
